{
    "name": "ali-habibian/sweetalert2-laravel",
    "description": "This package provides an easy-to-use integration for SweetAlert2 notifications in Laravel applications.",
    "version": "1.0.0",
    "type": "package",
    "require": {
        "illuminate/support": "^11.34",
        "sweetalert2/sweetalert2": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AliHabibian\\Sweetalert2Laravel\\SwalLaAlertServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AliHabibian\\Sweetalert2Laravel\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
